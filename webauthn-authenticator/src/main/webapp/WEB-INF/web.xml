<web-app>
  <servlet>
    <servlet-name>WebAuthnServlet</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>javax.ws.rs.Application</param-name>
      <param-value>app.App</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>WebAuthnServlet</servlet-name>
    <url-pattern>/webauthn/*</url-pattern>
  </servlet-mapping>
</web-app>